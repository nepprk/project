    .searchContainer {
      position: fixed;
      top: 10%;
      left: 25%;    
      width: 50%;
      background-color: #00629b;
      box-shadow: 0 0 6px 0 rgba(0, 0, 0, 0.5);
      border-radius: 4px;
      text-align: center;
      padding: 20px 0 20px 0;
      overflow: hidden;
      z-index: 10001;    
    }  
    
    .searchGroup {
      width: 80%;
      margin: 20px auto;
    }
    
    .searchInput {
      font-family: 'Montserrat', sans-serif;        
      font-size: 14px;
      font-weight: 300;  
      color: #9b9b9b;     
    } 
       
    .searchGroupIcon {
      border: 1px solid #e87722;
      background-color: #e87722;
      color: #ffffff;
      width: 54px;
      height: 34px;
      cursor: pointer;
    }

    .searchGroupIcon::before {
      top: 7px;
      right: 20px;
      width: 16px;
      height: 16px;
      border-radius: 50%;
      border: 3px solid #ffffff;
    }       

    .searchGroupIcon::after {
      height: 3px;
      width: 8px;
      background: #ffffff;
      bottom: 11px;
      right: 16px;
      -webkit-transform: rotate(45deg);
      -moz-transform: rotate(45deg);
      -ms-transform: rotate(45deg);
      -o-transform: rotate(45deg);
      transform: rotate(45deg);
    }        
    
    .searchGroupIcon::before, 
    .searchGroupIcon::after {
      content: '';
      position: absolute;
      -webkit-transition: opacity 0.3s;
      -moz-transition: opacity 0.3s;
      transition: opacity 0.3s;;
      -webkit-backface-visibility: hidden;
      backface-visibility: hidden;
    }    

    .searchIconForMobile {
      top: 26px;
      right: 30px;
      width: 16px;
      height: 16px;
      cursor: pointer;
      color: #ffffff;
      position: absolute;
      font-size: 26px;
    }       
    
    .searchIconForMobileLink,
    .searchIconForMobileLink:visited,
    .searchIconForMobileLink:hover,
    .searchIconForMobileLink:active {
      color: #ffffff;
      text-decoration: none;
    }      
    
    .searchClose {
      position: absolute;
      top: 5px;
      right: 10px;      
      width: 20px;
      height: 20px;
      cursor: pointer;
      font-size: 20px;
    }    
    
    .searchCloseIcon {
      color: #ffffff;
    }
    
