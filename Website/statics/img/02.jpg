<!-- Seup up flexslider carousel, carousel thumbnails and resize column heights -->
<script>
    $(window).load(function(){

      // Toggle the main carousel container ()
      $('#mainCarouselContainer').show();

      // Setup main carousel thumbnails
      // The slider being synced must be initialized first
      var thumbnails = $('#thumbnails').flexslider({
        animation: "slide",
        controlNav: false,
        asNavFor: '#carousel',
        touch: true,
        slideshow: true,
        animationLoop: false, // this param needs to stay false or synching is affected
        slideshowSpeed: 7000,
        animationSpeed: 600,
        pauseOnAction: true,
        pauseOnHover: true,
        itemWidth: 150
      });

      // Setup main carousel
      var carousel = $('#carousel').flexslider({
        animation: "slide",
        controlNav: false,
        sync: "#thumbnails",
        touch: true,
        slideshow: true,
        animationLoop: true,
        slideshowSpeed: 7000,
        animationSpeed: 600,
        pauseOnAction: true,
        pauseOnHover: true
      });

      // Allow the carousel to init before trying to resize column heights
      setTimeout(() => {
       resizeColumns();
      }, 150);
      
    });

    var resizeTimer;
    $(window).on('resize', function(e) {
      clearTimeout(resizeTimer);
      resizeTimer = setTimeout(function() {
        resizeColumns();
      }, 20);
    });

    function resizeColumns () {
      var docWidth = $(document).width();
      // Medium devices and up
      if (docWidth >= 992){

        // Reset threeColum class divs back to threeColumnHeight
        $('.threeColumn').css('height', '650px');
        // Reset the right of carousel box to be the same height as the main carousel
        var carouselHeight = $('#mainCarouselContainer').height();
        $('.rightOfCarousel').height(carouselHeight);

      } else { // all other screens and devices smaller than 992 pixels 
        // Adjust height to fit new content length
        $('.rightOfCarousel').height('auto');
        $('.threeColumn').height('auto');
      }
    }
</script>

